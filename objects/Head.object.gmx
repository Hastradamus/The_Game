  <events>
    <event eventtype="0" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///variable initialization

/* headShake: to control 'recoil' of players head */
headShake = 0;
rot=0
col=c_white
myXScale=0
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="2" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///reset headShake

//headShake is set in headRecoil_script
headShake = 0;
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="8" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///draw the head
x=Player.headx
y=Player.heady

if(!global.paused){
rot = Player.pdir; /* the default rotation of the 
    sprite according to the players position and the mouse */
} 
    
if(instance_place(x,y,Warp)) myXScale=0//so the head isn't visible in the warp and we can just use the sprite
else var myXScale=1

var endWarp=instance_place(x,y,EndWarp)
if(endWarp!=noone&amp;&amp;endWarp.image_index&gt;10)var myXScale=0
/* headShake is a global variable for the head object which
is set in a script when a button is shot to control 'recoil' */
rot = rot + headShake;

col=Player.image_blend
if(Player.image_xscale!=0)//so that it doesn't draw the head when you're dead
draw_sprite_ext(
                 player_head, //sprite
                 -1, //subimage
                 x, //x
                 y+2, //y
                 myXScale, //x scale
                 Player.playerDir, //yscale
                 rot, //roation
                 col, //image color
                 1 // alpha
                 )
</string>
          </argument>
        </arguments>
      </action>
    </event>
  </events>
  <PhysicsObject>0</PhysicsObject>
  <PhysicsObjectSensor>0</PhysicsObjectSensor>
